<h1>Welcome to <%= @room.name %></h1>
<div class="container">
  <% @messages.each do |message| %>
  <%= render 'message', message: message %>
  <%end%>

  <%= form_for [@room, Message.new] do |f| %>
    <%= f.label 'Username'  %>
    <%= f.text_field :username %>
    <%= f.label 'Content'  %>
    <%= f.text_field :content %>
    <%= f.submit %>
  <%end%>



</div>
